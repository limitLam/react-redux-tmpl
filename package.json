{
  "name": "react-antd-boilerplate",
  "version": "1.0.0",
  "description": "React + Antd 前端项目模板（支持IE8+）",
  "main": "config/webpack.dev",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev --progress --colors --inline --hot --open",
    "dll": "webpack --config config/webpack.dll --progress --profile --colors",
    "bundle": "webpack --config config/webpack.prod --progress --profile --colors",
    "polyfill": "webpack config/ie8-polyfills.js polyfill/ie8-polyfills-min.js -p",
    "build": "node config/build"
  },
  "author": "frontui",
  "license": "ISC",
  "dependencies": {
    "antd": "^1.11.2",
    "classnames": "^2.2.5",
    "console-polyfill": "^0.2.3",
    "core-js": "^2.4.1",
    "es5-shim": "^4.5.9",
    "es6-promise": "^3.3.1",
    "fetch-ie8": "^1.4.3",
    "history": "^2.1.2",
    "html5shiv": "^3.7.3",
    "immutable": "^3.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.5",
    "react-router": "^1.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "es3ify-loader": "^0.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.13.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.1",
    "why-did-you-update": "0.0.8"
  }
}
